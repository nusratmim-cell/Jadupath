{
  "name": "teacher-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "node server.mjs",
    "dev:network": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "node scripts/apply-migration.js",
    "db:seed": "node scripts/seed-subjects.js",
    "upload:chapters": "node scripts/upload-chapters.js",
    "upload:training": "node scripts/upload-training.js",
    "validate": "node scripts/validate-data.js",
    "storage:setup": "node scripts/setup-storage.js",
    "organize": "node scripts/organize-content.js",
    "upload:storage:images": "node scripts/upload-images-to-storage.js",
    "upload:storage:pdfs": "node scripts/upload-pdfs-to-storage.js",
    "upload:storage:videos": "node scripts/upload-videos-to-storage.js",
    "upload:storage:all": "npm run upload:storage:images && npm run upload:storage:pdfs && npm run upload:storage:videos",
    "update:storage:urls": "node scripts/update-topic-urls.js",
    "verify:storage": "node scripts/verify-storage.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.93.3",
    "next": "16.1.4",
    "openai": "^6.16.0",
    "pdf-parse": "^2.4.5",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tesseract.js": "^7.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5"
  }
}
